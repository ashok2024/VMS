    <script>
        $(document).ready(function () {
            $().ready(function () {
                debugger;

                $('[data-toggle="tooltip"]').tooltip();
                // for set menu class active and manage collapse part
                var page = "@ViewBag.ActivePage";
                var UserType = "@Convert.ToString(Session["LoggedUserType"])";
                var identifier = ''; var collapseanchor = '';var collapseli = ''
                   if (page.includes("VisitorDashboard")) {
                        identifier = "1";
                    }
                      if (page.includes("VisitorList")) {
                            identifier = "2";
                        }
                        if (page == "CourierList") {
                            if (UserType == "Reception") {
                                identifier = "3";
                            }
                            else {
                                identifier = "23";
                            }
                        }
                        if (page == "VisitedVisitorList") {
                            identifier = "4";
                            collapseli = 'liuserReports'
                            collapseanchor = 'userReports'
                        }
                         if (page == "RejectedVisitorList") {
                            identifier = "5";
                            collapseli = 'liuserReports'
                            collapseanchor = 'userReports'
                        }
                      if (page == "EmployeeDashboard") {
                            identifier = "11";
                        }
                    if (page == "EmployeeVisitorList") {
                        identifier = "12";
                        }
                     if (page.includes("ScheduledVisitorList")) {
                            identifier = "13";
                        }
                        if (page.includes("EmployeeVisitedVisitorList")) {
                            identifier = "14";
                            collapseli = 'liEmpReports'
                            collapseanchor = 'EmpReports'
                            }
                         if (page.includes("EmployeeRejectedVisitorList")) {
                                identifier = "15";
                                collapseli = 'liEmpReports'
                                collapseanchor = 'EmpReports'
                            }
                        if (page.includes("AdminDashboard")) {
                            identifier = "21";
                        }
                        if (page =="Employee") {
                            identifier = "22";
                            collapseli = 'limaster'
                            collapseanchor = 'masterforms'
                        }
                        if (page == "AdminCourierList") {
                                    identifier = "23";
                            }
                        if (page == "DeviceEmployee") {
                                identifier = "33";
                            }
                         if (page == "UploadEmployee") {
                                identifier = "44";
                            }
                        if (page == "CompanyList") {
                            identifier = "24";
                            collapseli = 'limaster'
                            collapseanchor = 'masterforms'
                        }
                      if (page == "DepartmentList") {
                            identifier = "25";
                            collapseli = 'limaster'
                            collapseanchor = 'masterforms'
                        }
                        if (page == "BranchList") {
                            identifier = "26";
                            collapseli = 'limaster'
                            collapseanchor = 'masterforms'
                            }
                            if (page == "DesignationList") {
                                identifier = "27";
                                collapseli = 'limaster'
                                collapseanchor = 'masterforms'
                                }

