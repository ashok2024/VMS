                                    collapseli = 'lisetting'
                                    collapseanchor = 'settingforms'
                                }
                                if (page == "DeviceLogList") {
                                    identifier = "43";
                                    collapseli = 'lisetting'
                                    collapseanchor = 'settingforms'
                                    }
                                  if (page == "ApiMonitor") {
                                        identifier = "444";
                                        collapseli = 'lisetting'
                                        collapseanchor = 'settingforms'
                                    }


                $(".nav li a").removeClass("custom-active");

                $('#' + identifier).addClass('custom-active');
                $('#21').addClass('custom-active');
                $('#' + collapseanchor).addClass('custom-active');
                $('#' + collapseli).addClass('menu-open');

